package core;

templ PageTransfersChart() {
	@Layout("/transfers/chart", TransfersChartContent())
}

templ TransfersChartContent() {
	<main class="flex-1 flex flex-col">
		@Header("Chart")
		<div class="flex-1 p-6 overflow-auto bg-base-50">
			<div id="main" style="width: 100%; height: 100%;"></div>
			<script src="/static/public/echarts.min.js"></script>
			<script src="/static/public/chart-transfers.js"></script>
		</div>
	</main>
}
